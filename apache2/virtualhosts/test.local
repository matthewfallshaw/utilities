<VirtualHost *:80>
  ServerName test.local

  RewriteEngine On
  RewriteLog /Users/matt/apache2/log/mod_rewrite.log
  RewriteLogLevel 3
  RewriteMap test_map txt:/Users/matt/apache2/virtualhosts/test/rewritemap.txt
  RewriteCond ${test_map:%{REQUEST_URI}} !=""
  RewriteRule ^(.*)$ ${test_map:$1} [R=permanent,L]
  Include /Users/matt/apache2/virtualhosts/test/rewriterules.txt

  DocumentRoot /Users/matt/tmp/test.local/
</VirtualHost>
<Directory "/Users/matt/tmp/test.local/">
  Options Indexes MultiViews
  AllowOverride None
  Order allow,deny
  Allow from all
</Directory>
